            .col-sm-6.col-md-6
              img.wow.fadeInRight alt="" data-wow-duration="0.80s" src="../assets/img/features/img2.png" /
      .feature-list.item-3
        .container
          .row
            .col-sm-6.col-md-6
              img.wow.fadeInLeft alt="" data-wow-duration="0.80s" src="../assets/img/features/img3.png" /
            .col-md-6
              .feature-content.wow.fadeInRight data-wow-duration="0.80s"
